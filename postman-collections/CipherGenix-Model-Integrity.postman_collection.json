            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/theft-detection/{{model_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "theft-detection", "{{model_id}}"]
            }
          },
          "response": []
