{
  "info": {
    "name": "CipherGenix Model Integrity Service",
    "description": "AI model fingerprinting, monitoring, and theft detection",
    "version": "1.0.0",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8082",
      "type": "string"
    },
    {
      "key": "auth_token",
      "value": "",
      "type": "string"
    },
    {
      "key": "model_id",
      "value": "model_001",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Health & Info",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/health",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "health"]
            }
          },
          "response": []
        },
        {
          "name": "Service Info",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/info",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "info"]
            }
          },
          "response": []
        },
        {
          "name": "Dashboard",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/dashboard",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "dashboard"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Model Fingerprinting",
      "item": [
        {
          "name": "Create Model Fingerprint",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"modelId\": \"{{model_id}}\",\n  \"modelName\": \"CipherGenix Threat Detection Model\",\n  \"modelVersion\": \"1.0.0\",\n  \"parameters\": {\n    \"layer1_weights\": 1024,\n    \"layer2_weights\": 512,\n    \"layer3_weights\": 256,\n    \"output_layer\": 10,\n    \"total_parameters\": 1802\n  },\n  \"weightsHash\": {\n    \"layer1\": \"sha256:abc123def456\",\n    \"layer2\": \"sha256:def456ghi789\",\n    \"layer3\": \"sha256:ghi789jkl012\",\n    \"output\": \"sha256:jkl012mno345\"\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/fingerprint",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "fingerprint"]
            }
          },
          "response": []
        },
        {
          "name": "Get Model Fingerprint",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/fingerprint/{{model_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "fingerprint", "{{model_id}}"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Integrity Verification",
      "item": [
        {
          "name": "Verify Model Integrity",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"currentParameters\": {\n    \"layer1_weights\": 1024,\n    \"layer2_weights\": 512,\n    \"layer3_weights\": 256,\n    \"output_layer\": 10,\n    \"total_parameters\": 1802\n  },\n  \"currentWeightsHash\": {\n    \"layer1\": \"sha256:abc123def456\",\n    \"layer2\": \"sha256:def456ghi789\",\n    \"layer3\": \"sha256:ghi789jkl012\",\n    \"output\": \"sha256:jkl012mno345\"\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/verify/{{model_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "verify", "{{model_id}}"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Model Monitoring",
      "item": [
        {
          "name": "Start Model Monitoring",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/monitor/{{model_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "monitor", "{{model_id}}"]
            }
          },
          "response": []
        },
        {
          "name": "Stop Model Monitoring",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/monitor/{{model_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "monitor", "{{model_id}}"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Performance Monitoring",
      "item": [
        {
          "name": "Get Performance Metrics (7 days)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/performance/{{model_id}}?days=7",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "performance", "{{model_id}}"],
              "query": [
                {
                  "key": "days",
                  "value": "7"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Performance Metrics (30 days)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/v1/model-integrity/performance/{{model_id}}?days=30",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "model-integrity", "performance", "{{model_id}}"],
              "query": [
                {
                  "key": "days",
                  "value": "30"
                }
              ]
            }
          },
          "response": []
        }
      ]
    },
