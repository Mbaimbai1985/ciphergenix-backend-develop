    {
      "name": "3. Model Security Setup",
      "item": [
        {
          "name": "Generate Encryption Key",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"keyType\": \"AES\",\n  \"keySize\": 256,\n  \"purpose\": \"MODEL_ENCRYPTION\",\n  \"metadata\": {\n    \"owner\": \"demo_user\",\n    \"environment\": \"demo\",\n    \"model_id\": \"{{model_id}}\"\n  }\n}"
            },
            "url": {
              "raw": "{{security_engine_url}}/api/v1/security-engine/keys/generate",
              "host": ["{{security_engine_url}}"],
              "path": ["api", "v1", "security-engine", "keys", "generate"]
            }
          },
          "response": []
        },
