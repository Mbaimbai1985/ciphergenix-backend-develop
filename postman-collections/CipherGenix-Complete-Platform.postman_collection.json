              "raw": "{\n  \"sessionId\": \"demo_poisoning_{{$timestamp}}\",\n  \"datasetId\": \"demo_dataset_001\",\n  \"dataPoints\": [\n    {\n      \"id\": \"normal_001\",\n      \"features\": [0.2, 0.8, 0.3, 0.7],\n      \"label\": \"benign\",\n      \"timestamp\": \"2024-01-15T10:00:00Z\"\n    },\n    {\n      \"id\": \"suspicious_001\",\n      \"features\": [0.9, 0.1, 0.95, 0.05],\n      \"label\": \"benign\",\n      \"timestamp\": \"2024-01-15T10:01:00Z\"\n    },\n    {\n      \"id\": \"normal_002\",\n      \"features\": [0.3, 0.7, 0.4, 0.6],\n      \"label\": \"benign\",\n      \"timestamp\": \"2024-01-15T10:02:00Z\"\n    }\n  ],\n  \"modelType\": \"CLASSIFICATION\",\n  \"detectionAlgorithms\": [\"ISOLATION_FOREST\", \"LOF\", \"STATISTICAL_OUTLIER\"]\n}"
